body{margin:0px; padding:0px; color:#000; background:url(../images/body_bg.png) #FFF repeat-x; font-family:Arial, Helvetica, sans-serif; font-size:12px; position:relative;}
a, img{border:none; outline:none; text-decoration:none;}
h1, h2, h3, h4, h5, h5, p, ul, li{margin:0px; padding:0px; list-style-type:none;}
.fleft{float:left;}
.fright{float:right;}
.clear{clear:both;}
.pad-10{padding:10px;}
.txt-left{text-align:left;}
.txt-right{text-align:right;}



.wrapper{width:980px; margin:0 auto;}
#header{width:980px; background:url(../images/shadow_top.png) no-repeat center 7px;}
#logo{padding:15px 0 10px 0;}
*:first-child+html #logo{padding:13px 0 8px 0;}
.translate {padding:42px 0 0 495px;}

/* Main Navigation Start */
#main-navigation{padding:5px 0 0 0;}
#main-nav{/*width:800px; height:50px; */position:relative; z-index:100; list-style:none; padding:0;}
#main-nav .main-nav-item{display:inline;}
#main-nav .main-nav-tab{float:left; padding:10px 10px 10px 10px; color:#268bc1; font-size:13px; font-weight:bold; display:block;}
#main-nav .main-nav-item-active .main-nav-tab{color:#FFF; background:#176fa0;}
#main-nav .main-nav-dd{position:absolute; top:35px; left:0; margin:0; padding:0; background-color:#FFF; border-bottom:4px solid #176fa0; display:none;}
#main-nav .main-nav-item-active .main-nav-dd{display:block;}
#main-nav .main-nav-dd-column{width:130px; padding:15px 20px 8px; display:table-cell; border-left:1px solid #176fa0;  *float:left;  *border-left:0;}
#main-nav .main-nav-dd-column:first-child{border-left:0;}
#main-nav .main-nav-dd h3{font-size:14px; color:#2689c0; padding:0 0 7px 0;}
#main-nav .main-nav-dd a{color:#777; text-decoration:none;}
#main-nav .main-nav-dd a:hover{color:#176fa0; text-decoration:underline;}
#main-nav .main-nav-dd ul{list-style:none; padding:0;}
#main-nav .main-nav-dd hr{border:1px dotted #ddd;}
/* Main Navigation End */

/* Banner Start */
#banner-wrap{background:url(../images/banner_bg.png) repeat-x; padding:0 0 10px 0}
.banner-content{padding:10px;}
.container {height:300px; overflow:hidden; position:relative; cursor:pointer; }
.slides { position:absolute; top:0; left:0; }
.slides > div { position:absolute; top:0; width:980px; display:none; }
#loopedSlider,#newsSlider { margin:0 auto; width:980px; position:relative; clear:both; }
ul.pagination { list-style:none; padding:10px; background:#FFF; bottom:0; -webkit-border-top-left-radius:5px; -webkit-border-top-right-radius:5px; -moz-border-radius-topleft:5px; -moz-border-radius-topright:5px; border-top-left-radius:5px; border-top-right-radius:5px; width:100px; margin:0 auto; height:28px; display:block; }
ul.pagination li  { float:left; padding:0 1px; }
ul.pagination li a { padding:0px 0px; display:block; background:url(../images/banner_bullets.png) no-repeat; width:15px; height:15px; text-indent:-9999px; }
ul.pagination li a:hover, ul.pagination li.active a:hover{background:url(../images/banner_bullets.png) no-repeat bottom;}
ul.pagination li.active a { background:url(../images/banner_bullets.png) no-repeat bottom; color:white; text-indent:-999px; }	
.slider{float:left; width:980px; height:324px; position:relative; z-index:1;}
.sliderContent{float:left; width:980px; height:324px; clear:both; position:relative; overflow:hidden;}
.sliderArrows a{display:block; text-indent:-9999px; outline:none; z-index:50; background-image:url(../images/prev_next.png); width:42px; height:42px; position:absolute; top:50%; margin-top:-34px;}
.prev{background-position:0 0; left:15px;}
.prev:hover{background-position:0 -44px;}
.next{right:15px; background-position:-44px 0;}
.next:hover{background-position:-44px -44px;}
.sliderContent .item{position:absolute; width:980px; height:350px; background:url(../images/banner_abstract.png) no-repeat; padding:20px 0;}
.sliderBullets{position:absolute; bottom:0; left:45%; z-index:50; /*margin-left:-45px;*/ padding:10px; background:#FFF; -webkit-border-top-left-radius:5px; -webkit-border-top-right-radius:5px; -moz-border-radius-topleft:5px; -moz-border-radius-topright:5px; border-top-left-radius:5px; border-top-right-radius:5px;}
.sliderBullets a{display:block; float:left; text-indent:-9999px; margin-left:5px; outline:none; width:15px; height:15px; background:url(../images/banner_bullets.png) no-repeat 0px 0;}
.sliderBullets .active{background-position:0 -17px;}
.sliderContent a{outline:none;}
.banner-txt{width:580px;}
.banner-txt h1{color:#FFF; font-family:"Trebuchet MS", Arial, Helvetica, sans-serif; font-size:30px; padding:0 0 20px 0}
.banner-txt h1 span{color:#0d3f54; font-size:24px; display:block; padding-top:10px;}
.banner-txt p{padding:0 50px 10px 0; color:#FFF; width:400px; line-height:25px;}
.banner-img{width:386px;}
/* Banner End */

