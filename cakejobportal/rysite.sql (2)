-- --------------------------------------------------------

-- 
-- Table structure for table `job_bookmark_jobseeker`
-- 

CREATE TABLE `job_bookmark_jobseeker` (
  `book_id` int(9) NOT NULL AUTO_INCREMENT,
  `seeker_id` int(9) NOT NULL,
  `ad_id` int(9) NOT NULL,
  `job_type` int(1) NOT NULL DEFAULT '1',
  PRIMARY KEY (`book_id`)
) ENGINE=MyISAM AUTO_INCREMENT=950 DEFAULT CHARSET=latin1 AUTO_INCREMENT=950 ;

