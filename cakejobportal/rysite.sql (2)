-- --------------------------------------------------------

-- 
-- Table structure for table `job_rec_payments`
-- 

CREATE TABLE `job_rec_payments` (
  `pay_id` int(11) NOT NULL AUTO_INCREMENT,
  `invoice_id` int(11) NOT NULL,
  `pay_date` date NOT NULL,
  `paid_by` varchar(30) NOT NULL,
  `pay_amount` double NOT NULL,
  `cheque_number` varchar(20) NOT NULL,
  `cheque_date` date NOT NULL,
  `bank` varchar(30) NOT NULL,
  `branch` varchar(30) NOT NULL,
  `pay_status` tinyint(4) NOT NULL,
  `received_date` date NOT NULL,
  `bounce_date` date NOT NULL,
  `account_number` varchar(200) NOT NULL,
  `post_to` varchar(200) NOT NULL,
  `payable` varchar(200) NOT NULL,
  `ref` varchar(200) NOT NULL,
  PRIMARY KEY (`pay_id`)
) ENGINE=MyISAM AUTO_INCREMENT=548 DEFAULT CHARSET=latin1 AUTO_INCREMENT=548 ;

