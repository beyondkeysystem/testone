-- 
-- Table structure for table `trans_member`
-- 

CREATE TABLE `trans_member` (
  `id` int(10) NOT NULL AUTO_INCREMENT,
  `user_id` int(11) NOT NULL,
  `unique_id` varchar(100) NOT NULL,
  `first_name` varchar(100) NOT NULL,
  `last_name` varchar(100) NOT NULL,
  `address` text NOT NULL,
  `city` varchar(100) NOT NULL,
  `state` varchar(100) NOT NULL,
  `zip` varchar(100) NOT NULL,
  `country_id` varchar(4) NOT NULL,
  `email` varchar(150) NOT NULL,
  `paypalID` varchar(150) NOT NULL,
  `contact_no` varchar(200) NOT NULL,
  `about_me` longtext NOT NULL,
  `user_name` varchar(50) NOT NULL,
  `password` varchar(50) NOT NULL,
  `profile_image_file` varchar(255) NOT NULL,
  `shipping_fname` varchar(100) NOT NULL,
  `shipping_lname` varchar(100) NOT NULL,
  `shipping_address` varchar(200) NOT NULL,
  `shipping_country` varchar(100) NOT NULL,
  `shipping_pcode` varchar(50) NOT NULL,
  `billing_fname` varchar(100) NOT NULL,
  `billing_lname` varchar(100) NOT NULL,
  `billing_address` varchar(200) NOT NULL,
  `billing_country` varchar(100) NOT NULL,
  `billing_pcode` varchar(50) NOT NULL,
  `player_preference_height` int(4) NOT NULL,
  `player_preference_width` int(4) NOT NULL,
  `player_preference_border_style` int(4) NOT NULL,
  `player_preference_is_loop` enum('Y','N') NOT NULL,
  `is_banned` enum('Y','N') NOT NULL DEFAULT 'N',
  `last_login` int(11) NOT NULL,
  `last_logout` int(11) NOT NULL,
  `customised_space` int(11) NOT NULL DEFAULT '0',
  `customised_bw` int(11) NOT NULL DEFAULT '0',
  `is_admin_viewed` char(1) NOT NULL DEFAULT 'n',
  `show_me_as_online` char(1) NOT NULL DEFAULT 'n',
  `sale_percentage` double NOT NULL,
  `user_role` enum('user','admin') NOT NULL DEFAULT 'user',
  PRIMARY KEY (`id`),
  UNIQUE KEY `id` (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=740 DEFAULT CHARSET=latin1 AUTO_INCREMENT=740 ;

