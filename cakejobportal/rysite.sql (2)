-- --------------------------------------------------------

-- 
-- Table structure for table `job_user`
-- 

CREATE TABLE `job_user` (
  `user_id` int(9) NOT NULL AUTO_INCREMENT,
  `user_name` varchar(200) NOT NULL DEFAULT '',
  `user_password` varchar(200) NOT NULL DEFAULT '',
  `user_first_name` varchar(200) NOT NULL DEFAULT '',
  `user_last_name` varchar(200) NOT NULL DEFAULT '',
  `user_email` varchar(200) NOT NULL DEFAULT '',
  `user_phone` varchar(200) NOT NULL,
  `user_copyright` varchar(200) NOT NULL,
  `user_status` int(10) NOT NULL DEFAULT '0',
  PRIMARY KEY (`user_id`)
) ENGINE=MyISAM AUTO_INCREMENT=2 DEFAULT CHARSET=latin1 AUTO_INCREMENT=2 ;

-- 
-- Dumping data for table `job_user`
-- 

INSERT INTO `job_user` VALUES (1, 'job', 'portal', 'Dina', 'Douglas', 'info@recruityoung.com', '+004 (0)00 508 7516.', ' Recruit Young 2014', 1);

-- --------------------------------------------------------

-- 
-- Table structure for table `notifications`
-- 

CREATE TABLE `notifications` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `employer` int(11) NOT NULL,
  `user_id` int(11) NOT NULL,
  `notication_id` int(11) NOT NULL,
  `type` varchar(255) NOT NULL,
  `table_name` varchar(255) NOT NULL,
  `url` varchar(255) NOT NULL,
  `message` text NOT NULL,
  `icon_image` varchar(255) NOT NULL,
  `user_name` varchar(255) NOT NULL,
  `noti_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `status` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=16 DEFAULT CHARSET=latin1 AUTO_INCREMENT=16 ;

