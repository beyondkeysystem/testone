-- --------------------------------------------------------

-- 
-- Table structure for table `job_ad_temp`
-- 

CREATE TABLE `job_ad_temp` (
  `ad_id` int(11) NOT NULL AUTO_INCREMENT,
  `position_name` varchar(100) NOT NULL,
  `vacansy` varchar(50) NOT NULL,
  `company_name` varchar(50) NOT NULL,
  `company_desc` varchar(250) NOT NULL,
  `department` varchar(200) NOT NULL,
  `grade` varchar(200) NOT NULL,
  `application_form` varchar(100) NOT NULL,
  `date_of_taking` date NOT NULL,
  `offered_package` varchar(100) NOT NULL,
  `off_package1` varchar(100) NOT NULL,
  `off_package2` varchar(100) NOT NULL,
  `off_package3` varchar(100) NOT NULL,
  `off_package4` varchar(100) NOT NULL,
  `off_package5` varchar(100) NOT NULL,
  `off_package6` varchar(100) NOT NULL,
  `off_package_benefits` text NOT NULL,
  `off_package_b1` varchar(100) NOT NULL,
  `qks_1` varchar(250) NOT NULL,
  `qks_2` varchar(250) NOT NULL,
  `qks_3` varchar(250) NOT NULL,
  `qks_4` varchar(250) NOT NULL,
  `qks_5` varchar(250) NOT NULL,
  `qks_6` varchar(250) NOT NULL,
  `qks_7` varchar(250) NOT NULL,
  `qks_8` varchar(250) NOT NULL,
  `qks_9` varchar(250) NOT NULL,
  `qks_10` varchar(250) NOT NULL,
  `qks_11` varchar(250) NOT NULL,
  `preference_emp_1` text NOT NULL,
  `preference_emp_2` text NOT NULL,
  `preference_emp_3` text NOT NULL,
  `adFrom` date NOT NULL,
  `adTo` date NOT NULL,
  `level` varchar(50) NOT NULL,
  `salary_from` double NOT NULL,
  `salary_to` double NOT NULL,
  `salary_type` varchar(20) NOT NULL,
  `town` varchar(50) NOT NULL,
  `type` varchar(20) NOT NULL,
  `responsiblities1` varchar(50) NOT NULL,
  `responsiblities2` varchar(50) NOT NULL,
  `responsiblities3` varchar(50) NOT NULL,
  `responsiblities4` varchar(50) NOT NULL,
  `responsiblities5` varchar(50) NOT NULL,
  `min_sec_qualification` varchar(50) NOT NULL,
  `min_ter_qualification` varchar(50) NOT NULL,
  `exp_required` varchar(50) NOT NULL,
  `preference` varchar(50) NOT NULL,
  `min_comp_literacy` varchar(50) NOT NULL,
  `special_comp_skill` varchar(50) NOT NULL,
  `drivers_license` varchar(50) NOT NULL,
  `behaviour` varchar(50) NOT NULL,
  `registered_associations` varchar(100) NOT NULL,
  `display_info` varchar(20) NOT NULL,
  `ques1` varchar(200) NOT NULL,
  `ques2` varchar(200) NOT NULL,
  `ques3` varchar(200) NOT NULL,
  `ques4` varchar(200) NOT NULL,
  `ques5` varchar(200) NOT NULL,
  `filter_gender` varchar(10) NOT NULL,
  `filter_equity_status` varchar(50) NOT NULL,
  `filter_citizenship` varchar(50) NOT NULL,
  `filter_town` varchar(50) NOT NULL,
  `filter_drivers_license` varchar(50) NOT NULL,
  `filter_pa` varchar(50) NOT NULL,
  `filter_endorsement` varchar(100) NOT NULL,
  `filter_x_vehicle` varchar(50) NOT NULL,
  `filter_x_driver` varchar(50) NOT NULL,
  `filter_criminal_record` varchar(50) NOT NULL,
  `filter_ter_qualification` varchar(50) NOT NULL,
  `filter_sec_qualification` varchar(50) NOT NULL,
  `confirm` tinyint(4) NOT NULL,
  `job_desc` text NOT NULL,
  `job_benefits` text NOT NULL,
  `job_skills` varchar(200) NOT NULL,
  `experience` int(11) NOT NULL,
  `bursary_name` varchar(250) NOT NULL,
  `bursary_year` varchar(250) NOT NULL,
  `bursary_details_offer` varchar(250) NOT NULL,
  `bursary_industry` varchar(250) NOT NULL,
  `bursary_institution` varchar(250) NOT NULL,
  `bursary_req_study` varchar(250) NOT NULL,
  `bursary_qualification` varchar(250) NOT NULL,
  `bursary_level` varchar(250) NOT NULL,
  `bursary_study` varchar(250) NOT NULL,
  `bursary_offer` varchar(250) NOT NULL,
  `bursary_inclusive` varchar(250) NOT NULL,
  `bursary_estimated_annual_value` varchar(250) NOT NULL,
  `bursary_entry_req` text NOT NULL,
  `bursary_con_granting` text NOT NULL,
  `seeker_work_type` varchar(250) NOT NULL,
  `seeker_vacation_type` varchar(250) NOT NULL,
  `job_attach` varchar(250) NOT NULL,
  `seeker_work_cat` varchar(250) NOT NULL,
  `seeker_work_subcat` varchar(250) NOT NULL,
  `available_month_to` date NOT NULL,
  `available_month_from` date NOT NULL,
  `type_of_part_time_work` varchar(250) NOT NULL,
  `available_days` varchar(250) NOT NULL,
  `available_assume_duties` date NOT NULL,
  PRIMARY KEY (`ad_id`)
) ENGINE=MyISAM AUTO_INCREMENT=2 DEFAULT CHARSET=latin1 AUTO_INCREMENT=2 ;

