-- --------------------------------------------------------

-- 
-- Table structure for table `job_comment`
-- 

CREATE TABLE `job_comment` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `comment_author` varchar(500) NOT NULL,
  `comment_author_email` varchar(500) NOT NULL,
  `comment_author_url` varchar(500) NOT NULL,
  `comment_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `comment_content` text NOT NULL,
  `comment_approved` int(11) NOT NULL,
  `comment_type` varchar(500) NOT NULL,
  `comment_parent` varchar(500) NOT NULL,
  `user_id` int(50) NOT NULL,
  `status` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=34 DEFAULT CHARSET=latin1 AUTO_INCREMENT=34 ;

