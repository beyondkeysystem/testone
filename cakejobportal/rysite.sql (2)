-- --------------------------------------------------------

-- 
-- Table structure for table `job_rec_payment_plans`
-- 

CREATE TABLE `job_rec_payment_plans` (
  `plan_id` int(11) NOT NULL AUTO_INCREMENT,
  `plan_name` varchar(100) NOT NULL,
  `unlimited_job_ads` int(9) NOT NULL,
  `system_shortlisting` int(9) NOT NULL,
  `regret_function` int(9) NOT NULL,
  `client_talent` int(9) NOT NULL,
  `full_access_jobseekers` int(9) NOT NULL,
  `client_logo` int(9) NOT NULL,
  `intercompany_ad` int(9) NOT NULL,
  `rate` double NOT NULL,
  `positions` int(11) NOT NULL,
  `rate_per_position` double NOT NULL,
  `plan_status` int(9) NOT NULL,
  `type` varchar(20) NOT NULL,
  `number_job` int(11) NOT NULL,
  `rate_per_job` double NOT NULL,
  PRIMARY KEY (`plan_id`)
) ENGINE=MyISAM AUTO_INCREMENT=25 DEFAULT CHARSET=latin1 AUTO_INCREMENT=25 ;

