-- --------------------------------------------------------

-- 
-- Table structure for table `trans_member_membership_scheme`
-- 

CREATE TABLE `trans_member_membership_scheme` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `member_id` varchar(100) NOT NULL,
  `membership_scheme_id` int(10) NOT NULL,
  `order_no` varchar(100) NOT NULL,
  `order_date_time` datetime NOT NULL,
  `valid_from` date NOT NULL,
  `valid_upto` date NOT NULL,
  `fee_amount` double(11,2) NOT NULL,
  `is_auto_renew` enum('Y','N') NOT NULL,
  `is_payment_complete` tinyint(1) NOT NULL DEFAULT '0',
  `is_admin_alloted` char(1) NOT NULL DEFAULT 'N',
  `is_admin_viewed` char(1) NOT NULL DEFAULT 'n',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=555 DEFAULT CHARSET=latin1 AUTO_INCREMENT=555 ;

