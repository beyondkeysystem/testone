-- --------------------------------------------------------

-- 
-- Table structure for table `job_recruiter`
-- 

CREATE TABLE `job_recruiter` (
  `rec_id` int(9) NOT NULL AUTO_INCREMENT,
  `rec_reg_date` date NOT NULL,
  `rec_uid` varchar(20) NOT NULL DEFAULT '0',
  `rec_name` varchar(50) NOT NULL,
  `comp_name` varchar(50) NOT NULL,
  `comp_type` tinyint(4) NOT NULL DEFAULT '0',
  `company_desc` varchar(250) NOT NULL,
  `rec_address` varchar(200) NOT NULL DEFAULT '',
  `rec_postalcode` varchar(20) NOT NULL,
  `rec_city` varchar(20) NOT NULL,
  `rec_state` varchar(20) NOT NULL,
  `rec_country` varchar(20) NOT NULL,
  `rec_phone` varchar(20) NOT NULL,
  `rec_mobile` varchar(20) NOT NULL,
  `rec_fax` varchar(20) NOT NULL,
  `rec_email` varchar(50) NOT NULL,
  `rec_web` varchar(100) NOT NULL,
  `comp_logo` varchar(100) NOT NULL,
  `rec_hidename` int(9) NOT NULL DEFAULT '0',
  `rec_hideaddress` int(9) NOT NULL DEFAULT '0',
  `rec_hideemail` int(9) NOT NULL DEFAULT '0',
  `rec_hidecity` int(9) NOT NULL DEFAULT '0',
  `rec_hidetelno` int(9) NOT NULL DEFAULT '0',
  `rec_password` varchar(20) NOT NULL,
  `rec_heared` varchar(100) NOT NULL DEFAULT '',
  `rec_IP` varchar(15) NOT NULL,
  `business_street` varchar(250) NOT NULL,
  `business_street_num` int(11) NOT NULL,
  `business_suburb` varchar(250) NOT NULL,
  `business_city` varchar(250) NOT NULL,
  `business_country` varchar(250) NOT NULL,
  `postal_po_box` varchar(250) NOT NULL,
  `postal_private_bag` varchar(250) NOT NULL,
  `postal_post_code` varchar(250) NOT NULL,
  `postal_city` varchar(250) NOT NULL,
  `postal_country` varchar(250) NOT NULL,
  `postal_region` varchar(250) NOT NULL,
  `rec_status` int(9) NOT NULL DEFAULT '0',
  `rec_vat_regno` varchar(200) NOT NULL,
  `rec_referral` varchar(100) NOT NULL,
  `rec_skype` varchar(100) NOT NULL,
  `rec_postal_address` varchar(255) NOT NULL,
  `rec_another_email` varchar(255) NOT NULL,
  `detail_desc` text NOT NULL,
  `links_comp_blog` varchar(255) NOT NULL,
  `links_comp_facebook` varchar(255) NOT NULL,
  `links_comp_linkedin` varchar(255) NOT NULL,
  `links_comp_twitter` varchar(200) NOT NULL,
  `links_comp_rss` varchar(200) NOT NULL,
  `links_comp_other` varchar(255) NOT NULL,
  `comp_banner` varchar(255) NOT NULL,
  `comp_industry` varchar(255) NOT NULL,
  PRIMARY KEY (`rec_id`),
  UNIQUE KEY `rec_email` (`rec_email`)
) ENGINE=MyISAM AUTO_INCREMENT=434 DEFAULT CHARSET=latin1 AUTO_INCREMENT=434 ;

