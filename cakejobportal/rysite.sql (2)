-- --------------------------------------------------------

-- 
-- Table structure for table `job_rec_invoices`
-- 

CREATE TABLE `job_rec_invoices` (
  `invoice_id` int(11) NOT NULL AUTO_INCREMENT,
  `additional_position` int(9) NOT NULL,
  `invoice_type` tinyint(1) NOT NULL,
  `invoice_date` date NOT NULL,
  `rec_id` int(11) NOT NULL,
  `plan_name` varchar(100) NOT NULL,
  `unlimited_job_ads` tinyint(4) NOT NULL,
  `system_shortlisting` tinyint(4) NOT NULL,
  `regret_function` tinyint(4) NOT NULL,
  `client_talent` tinyint(4) NOT NULL,
  `full_access_jobseekers` tinyint(4) NOT NULL,
  `client_logo` tinyint(4) NOT NULL,
  `intercompany_ad` tinyint(4) NOT NULL,
  `rate` double NOT NULL,
  `positions` int(11) NOT NULL,
  `rate_per_position` double NOT NULL,
  `shortlist_rate_per_position` double NOT NULL,
  `shortlisted_jobseekers` int(11) NOT NULL,
  `invoice_status` tinyint(4) NOT NULL,
  `additional_rate` double NOT NULL,
  `vat` double NOT NULL,
  `number_job` int(11) NOT NULL,
  `remaining_job` int(9) NOT NULL,
  `rate_per_job` double NOT NULL,
  `additional_job` double NOT NULL,
  `additional_job_rate` double NOT NULL,
  PRIMARY KEY (`invoice_id`)
) ENGINE=MyISAM AUTO_INCREMENT=640 DEFAULT CHARSET=latin1 AUTO_INCREMENT=640 ;

