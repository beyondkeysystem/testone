-- 
-- Table structure for table `payments_table`
-- 

CREATE TABLE `payments_table` (
  `pay_id` int(11) NOT NULL AUTO_INCREMENT,
  `employer_id` int(11) NOT NULL COMMENT 'member id for employer and candidate both',
  `plan_id` int(11) NOT NULL,
  `pay_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `pay_by` varchar(220) NOT NULL,
  `pay_amount` varchar(20) NOT NULL,
  `start_date` date NOT NULL,
  `end_date` date NOT NULL,
  `duration` varchar(255) NOT NULL,
  `type` varchar(220) NOT NULL,
  `status` varchar(11) NOT NULL,
  PRIMARY KEY (`pay_id`)
) ENGINE=InnoDB AUTO_INCREMENT=28 DEFAULT CHARSET=latin1 AUTO_INCREMENT=28 ;

