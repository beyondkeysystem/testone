-- --------------------------------------------------------

-- 
-- Table structure for table `users`
-- 

CREATE TABLE `users` (
  `id` int(10) NOT NULL AUTO_INCREMENT,
  `name` varchar(255) DEFAULT NULL,
  `first_name` varchar(255) NOT NULL,
  `last_name` varchar(255) NOT NULL,
  `sex` enum('male','female') NOT NULL,
  `email` varchar(255) NOT NULL COMMENT '(''username'')',
  `password` varchar(255) NOT NULL,
  `mobile` int(15) DEFAULT NULL,
  `status` varchar(20) NOT NULL COMMENT '(''active'',''inactive'')',
  `type` enum('candidate','employer','admin') DEFAULT NULL COMMENT '(''patient'',''clinician'',''dme'')',
  `address` varchar(255) DEFAULT NULL,
  `dob` date NOT NULL,
  `city` varchar(255) NOT NULL,
  `country` varchar(255) NOT NULL,
  `company_name` varchar(255) NOT NULL,
  `office_address` text NOT NULL,
  `created_date` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `first_login` int(11) NOT NULL,
  `role_id` int(11) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=156 DEFAULT CHARSET=latin1 ROW_FORMAT=DYNAMIC AUTO_INCREMENT=156 ;

