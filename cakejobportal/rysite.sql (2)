-- --------------------------------------------------------

-- 
-- Table structure for table `job_appliedjobs`
-- 

CREATE TABLE `job_appliedjobs` (
  `applied_id` int(9) NOT NULL AUTO_INCREMENT,
  `applied_date` date NOT NULL,
  `ad_id` int(11) NOT NULL,
  `seeker_id` int(9) NOT NULL,
  `ans1` text NOT NULL,
  `ans2` text NOT NULL,
  `ans3` text NOT NULL,
  `ans4` text NOT NULL,
  `ans5` text NOT NULL,
  `coverletter` text NOT NULL,
  `biographical_score` double NOT NULL,
  `requirement_score` double NOT NULL,
  `total_score` double NOT NULL,
  `applied_status` tinyint(4) NOT NULL,
  `letter_id` int(9) NOT NULL,
  `talent` int(11) NOT NULL,
  `recruiter_id` int(11) NOT NULL,
  PRIMARY KEY (`applied_id`)
) ENGINE=MyISAM AUTO_INCREMENT=2871 DEFAULT CHARSET=latin1 AUTO_INCREMENT=2871 ;

