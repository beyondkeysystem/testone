-- --------------------------------------------------------

-- 
-- Table structure for table `trans_member_video`
-- 

CREATE TABLE `trans_member_video` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `category` int(4) NOT NULL,
  `member_id` varchar(100) DEFAULT NULL,
  `title` varchar(255) DEFAULT NULL,
  `description` text,
  `tags` text,
  `video_input_file` varchar(255) DEFAULT NULL,
  `video_output_file` varchar(255) DEFAULT NULL,
  `video_output_file_size` int(11) DEFAULT NULL,
  `video_output_file_2` varchar(255) NOT NULL,
  `video_output_file_3` varchar(255) NOT NULL,
  `video_thumbnail_file` varchar(255) DEFAULT NULL,
  `video_length` varchar(10) DEFAULT NULL,
  `upload_date_time` datetime DEFAULT NULL,
  `is_encoding_success` char(1) DEFAULT NULL,
  `encoding_error_message` text,
  `encoding_start_date_time` datetime NOT NULL,
  `encoding_date_time` datetime DEFAULT NULL,
  `status` tinyint(4) DEFAULT NULL,
  `video_status` enum('public','private') NOT NULL DEFAULT 'public',
  `video_property` enum('free','paid') NOT NULL,
  `rent_price` float NOT NULL,
  `download_price` float NOT NULL,
  `t_rent_price` float NOT NULL,
  `t_download_price` float NOT NULL,
  `is_approved` tinyint(1) NOT NULL,
  `unique_video_id` varchar(30) NOT NULL,
  `like_count` int(10) NOT NULL,
  `hitflics_choice` tinyint(1) NOT NULL DEFAULT '0',
  `top_trailer` tinyint(1) NOT NULL DEFAULT '0',
  `top_interview` tinyint(1) NOT NULL DEFAULT '0',
  `top_clip` tinyint(1) NOT NULL DEFAULT '0',
  `rate` int(11) DEFAULT NULL,
  `is_preview` int(11) NOT NULL DEFAULT '0',
  `is_preview_category` int(11) NOT NULL DEFAULT '0',
  `c_id` int(11) DEFAULT NULL,
  `is_voting` char(1) NOT NULL DEFAULT 'y',
  `rating_caution` int(11) NOT NULL DEFAULT '3',
  `is_hidden_member` int(11) NOT NULL DEFAULT '0',
  `is_hidden_admin` int(11) NOT NULL DEFAULT '0',
  `film_type_id` int(11) NOT NULL DEFAULT '1',
  `is_shortfilm_featured` int(11) NOT NULL DEFAULT '0',
  `show_on_core` char(1) NOT NULL DEFAULT 'y',
  `is_pilot_member` char(1) NOT NULL DEFAULT 'n',
  `is_pilot_admin` char(1) NOT NULL DEFAULT 'n',
  `is_admin_viewed` char(1) NOT NULL DEFAULT 'n',
  `top_video` tinyint(1) NOT NULL DEFAULT '0' COMMENT '1=active & 0=not',
  `people_top_video` tinyint(1) NOT NULL DEFAULT '0' COMMENT 'top video for people choice',
  `is_compete_show` tinyint(4) NOT NULL DEFAULT '0' COMMENT '1=yes, 0=no',
  `comp_tube` tinyint(4) NOT NULL DEFAULT '0',
  `embedcode` text NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=1449 DEFAULT CHARSET=latin1 AUTO_INCREMENT=1449 ;

