-- 
-- Table structure for table `job_shortlisted_jobseekers`
-- 

CREATE TABLE `job_shortlisted_jobseekers` (
  `shortlist_id` int(11) NOT NULL AUTO_INCREMENT,
  `invoice_id` int(11) NOT NULL,
  `recruiter_id` int(11) NOT NULL DEFAULT '0',
  `seeker_id` int(11) NOT NULL DEFAULT '0',
  `status` tinyint(4) NOT NULL,
  `applied_id` int(11) NOT NULL,
  `feedback` int(11) NOT NULL,
  PRIMARY KEY (`shortlist_id`)
) ENGINE=MyISAM AUTO_INCREMENT=224 DEFAULT CHARSET=latin1 AUTO_INCREMENT=224 ;

