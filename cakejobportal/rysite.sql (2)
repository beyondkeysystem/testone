CREATE TABLE `video_caution_rating` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `rate` varchar(10) NOT NULL,
  `image` varchar(255) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=7 DEFAULT CHARSET=latin1 AUTO_INCREMENT=7 ;

