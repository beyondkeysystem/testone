-- 
-- Table structure for table `job_cv`
-- 

CREATE TABLE `job_cv` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `email_address` int(11) NOT NULL,
  `cv_path` varchar(255) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=3 DEFAULT CHARSET=latin1 AUTO_INCREMENT=3 ;

