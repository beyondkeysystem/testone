-- --------------------------------------------------------

-- 
-- Table structure for table `fluxbb2_search_matches`
-- 

CREATE TABLE `fluxbb2_search_matches` (
  `post_id` int(10) unsigned NOT NULL DEFAULT '0',
  `word_id` int(10) unsigned NOT NULL DEFAULT '0',
  `subject_match` tinyint(1) NOT NULL DEFAULT '0',
  KEY `fluxbb2_search_matches_word_id_idx` (`word_id`),
  KEY `fluxbb2_search_matches_post_id_idx` (`post_id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

